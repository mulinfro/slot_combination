#性别=(男|女)(生|歌手|声)=> ("$1");
export sex1=(${歌手性别}) => request(性别="$1") <0.1>;
export sex1=(${操作}|听|放|播|点)(${歌手性别})(的|唱的|演唱的) => request(性别="$2") <0.3>;
export sex1=(${操作}|听|放|播|点)(${对象})(${歌手性别})(的|唱的|演唱的) => request(性别="$3",对象="$2") <0.3>;
export sex2=(${操作}|听|放|播|点)(男女合唱|男女对唱)的情歌 => request(性别="男生&女生",音乐类型="情歌") <0.3>;
